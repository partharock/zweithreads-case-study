<?xml version="1.0" encoding="utf-8"?>
<configuration description="Template Tradefed config for AAOS contacts cache tests">
    <option name="test-suite-tag" value="general-tests" />
    <option name="test-suite-tag" value="automotive-tests" />
    <option name="config-descriptor:metadata" key="component" value="aaos-contacts-cache" />

    <target_preparer class="com.android.tradefed.targetprep.suite.SuiteApkInstaller">
        <option name="cleanup-apks" value="true" />
        <option name="test-file-name" value="<MODULE_NAME>.apk" />
    </target_preparer>

    <test class="com.android.tradefed.testtype.AndroidJUnitTest">
        <option name="package" value="<TEST_PACKAGE_NAME>" />
        <option name="runner" value="androidx.test.runner.AndroidJUnitRunner" />
    </test>
</configuration>
